                          {
                            "commodityId": "4",
                            "commodityDuty": 0,
                            "totalCommodityTaxesAndFees": 0,
                            "commodityVAT": 90,
                            "totalCommodityDutyandTax": 90,
                            "commodityCurrencyCode": "GBP",
                            "isCalculable": true,
                            "hsCode": "8546901000"
                          }
                        ]
                      },
                      "alversion": 0,
                      "dpversion": null,
                      "transID": "325467165",
                      "error": null,
                      "perfStats": {
                        "absLayerTime": "139",
                        "fulfillTime": "ThuFeb027:56:53.231-05:002023",
