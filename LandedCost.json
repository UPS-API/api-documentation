                        "totalDutyandTax": 762,
                        "grandTotal": 786.65,
                        "importCountryCode": "GB",
                        "shipmentItems": [
                          {
                            "commodityId": "1",
                            "commodityDuty": 60,
                            "totalCommodityTaxesAndFees": 0,
                            "commodityVAT": 612,
                            "totalCommodityDutyandTax": 672,
                            "commodityCurrencyCode": "GBP",
                            "isCalculable": true,
                            "hsCode": "4009320090"
