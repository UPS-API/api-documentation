      "PreNotificationRequest_Shipment": {
        "type": "object",
        "maximum": 1,
        "required": [
          "ShipToAddress",
          "ShipFromAddress",
          "Service",
          "ShipperNumber",
          "RegulationSet",
          "Package",
          "ShipmentIdentificationNumber",
          "PickupDate"
        ],
        "properties": {
          "ShipperNumber": {
            "description": "Shipper's six digit account number.",
            "maximum": 1,
            "type": "string",
            "minLength": 6,
            "maxLength": 6
          },
          "ShipmentIdentificationNumber": {
            "description": "1Z Number of the first package in the shipment.",
